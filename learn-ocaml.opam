opam-version: "2.0"
name: "learn-ocaml-deps"
version: "~preview"
authors: [
  "Benjamin Canou (OCamlPro)"
  "Çağdaş Bozman (OCamlPro)"
  "Grégoire Henry (OCamlPro)"
]
maintainer: "Louis Gesbert"
license: "AGPLv3"
homepage: "https://github.com/ocaml-sf/learn-ocaml"
bug-reports: "https://github.com/ocaml-sf/learn-ocaml/issues"
depends: [
  "ocaml" {>= "4.05.0"}
  "ocamlfind" {build}
  "ocp-build" {build}
  "base64"
  "ezjsonm"
  "camlp4"
  "js_of_ocaml" {= "3.1.0" }
  "js_of_ocaml-camlp4"
  "js_of_ocaml-toplevel"
  "js_of_ocaml-compiler"
  "js_of_ocaml-lwt"
  "js_of_ocaml-tyxml"
  "lwt" {= "3.1.0"}
  "ocp-indent"
  "ocp-ocamlres"
  "ocplib-json-typed"
  "optcomp"
  "pprint"
  "ppx_tools"
  "react"
  "reactiveData"
  "tyxml"
  "base" {= "v0.9.4"}
  "cohttp" {= "0.99.0"}
  "cohttp-lwt-unix" {>= "0.99.0"}
  "magic-mime"
  "omd"
  "markup"
  "cmdliner"
  "uutf" {>= "1.0" }
  "easy-format" {>= "1.3.0" }
  "yojson" {>= "1.4.0" }
  "ppx_cstruct"
]
pin-depends: [
  ["ocp-indent.1.6.1" "git+https://github.com/OCamlPro/ocp-indent.git#master"]
  ["ocp-ocamlres.0.4" "git+https://github.com/OCamlPro/ocp-ocamlres.git"]
]
build: make
install: [
  ["cp" "-r" "www" _:share]
  ["cp" "_obuild/learnocaml-simple-server/learnocaml-simple-server.byte" bin]
]
